{
  "name": "eclipse-bot",
  "version": "0.7.1",
  "description": "Reddit Eclipse custom Discord bot",
  "dependencies": {
    "discord.js": "^11.3.2",
    "dotenv": "^7.0.0",
    "enmap": "^4.8.1",
    "express": "^4.16.3",
    "moment-timezone": "^0.5.21",
    "outdent": "^0.7.0",
    "twemoji": "^11.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^11.11.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "better-sqlite-pool": "^0.2.2",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:types && npm run test:unit",
    "test:lint": "eslint --ext .ts \".\"",
    "test:types": "tsc --project . --pretty",
    "test:unit": "mocha --require ts-node/register test/**/*.test.ts",
    "lint": "eslint --fix --ext .ts \".\"",
    "dev": "tsnd src/index.ts",
    "compile": "tsc --project .",
    "start": "npm run compile && node dist/index.js"
  },
  "repository": {
    "type": "git",

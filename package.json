{
  "name": "eclipse-bot",
  "version": "0.9.2",
  "license": "MIT",
  "author": "Paul Matthew Barrameda",
  "description": "Reddit Eclipse custom Discord bot",
  "repository": "https://github.com/Luis729/eclipse-bot",
  "dependencies": {
    "clash-of-clans-api": "^0.6.3",
    "discord.js": "^12.0.2",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "moment-timezone": "^0.5.28",
    "outdent": "^0.7.1",
    "twemoji": "^12.1.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "better-sqlite3": "^5.4.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "markdownlint-cli": "^0.22.0",
    "nodemon": "^2.0.2"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node --require dotenv/config src/index.js",
    "test": "npm run test:eslint && npm run test:markdownlint",
    "test:eslint": "eslint --report-unused-disable-directives src",
    "test:markdownlint": "markdownlint --config .markdownlint.yml README.md",
    "start": "npm run build && npm run serve",
    "build": "babel src --out-dir dist",
    "serve": "node --require dotenv/config dist/index.js"
  }
}
